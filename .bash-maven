alias mvnci="mvn clean install"
alias int="mvnci -Pintegrationtest"
alias mvnnotests="mvn clean install -DskipTests"
alias mvnquickstart="mvn archetype:generate -DarchetypeArtifactId=maven-archetype-quickstart"

alias jgitflow-feature-start="mvn jgitflow:feature-start"
alias jgitflow-feature-finish="mvn jgitflow:feature-finish"
alias jgitflow-release-start="mvn jgitflow:release-start"
alias jgitflow-release-finish="mvn jgitflow:release-finish"
alias jgitflow-hotfix-start="mvn jgitflow:hotfix-start"
alias jgitflow-hotfix-finish="mvn jgitflow:hotfix-finish"
alias jgitflow-build-number="mvn jgitflow:build-number"
